#!/bin/sh
# Sample service

start() {
echo "Started start service"
loadkmap < /etc/da.kmap
}

stop() {
echo "Stopped start service"
}

case "$1" in
    start)
       start
       ;;
    stop)
       stop
       ;;
    restart)
       stop
       start
       ;;
    status)
       # code to check status of app comes here
       # example: status program_name
       ;;
    *)
       echo "Usage: $0 {start|stop|status|restart}"
esac

exit $?
